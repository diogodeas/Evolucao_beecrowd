#include <stdio.h>
#include <string.h>
void testeparaimpressao(char saida[]) {
    int tamanho = strlen(saida), verifica = 0;
    for (int i = 0; i < tamanho; i++){
        if (!verifica && saida[i] != '0')
            verifica = 1;
        if (verifica)
            printf("%c", saida[i]);
    }
    if (!verifica)
        printf("0");
    printf("\n");
}
int main(){
    char entrada[10100], numeroquefalta, verifica=0, saida[10100];
    int i, j=0;
    while(1){
        j = 0;
        verifica = 0;
        scanf("%c", &numeroquefalta);
        scanf("%s", entrada);
        getchar();
        if(numeroquefalta == '0'&& entrada[0] == '0'){
            return 0;
        }
        for(i=0; i<strlen(entrada); i++){
            if(entrada[i]!=numeroquefalta){
                saida[j] = entrada[i];
                j++; 
            }
        }
        saida[j] = '\0';
        if(strlen(saida)==0){
            printf("0\n");
        }else{
            testeparaimpressao(saida);
        }
    }
    return 0; 
}
